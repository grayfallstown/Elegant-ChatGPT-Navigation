{
  "name": "elegant-chatgpt-navigation",
  "version": "2.0.3",
  "private": true,
  "description": "Navigate through long ChatGPT chats effortlessly",
  "scripts": {
    "build": "esbuild src/content.js --bundle --format=iife --outfile=dist/content.bundle.js",
    "build:dev": "esbuild src/content.js --bundle --format=iife --sourcemap --outfile=dist/content.bundle.js",
    "watch": "esbuild src/content.js --bundle --format=iife --sourcemap --watch --outfile=dist/content.bundle.js"
  },
  "devDependencies": {
    "esbuild": "^0.24.0"
  }
}