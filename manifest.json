{
  "manifest_version": 3,
  "name": "Elegant ChatGPT Navigation v9",
  "version": "2.0.0",
  "description": "Consolidated navigation panel with all features unified",
  "permissions": [],
  "host_permissions": [
    "https://chatgpt.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["dist/content.bundle.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icons/logo.png"
  }
}
